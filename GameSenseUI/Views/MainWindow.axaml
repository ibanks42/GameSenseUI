        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="GameSenseUI.Views.MainWindow"
        Title="GameSenseUI"
        Width="300"
        Height="400">

    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Spacing="20">
        <Button Click="Create_OnClick" Width="150" HorizontalContentAlignment="Center">Create Event</Button>
        <Button Click="Send_OnClick" Width="150" HorizontalContentAlignment="Center">Send Event</Button>
    </StackPanel>

</Window>